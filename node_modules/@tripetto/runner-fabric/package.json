{
  "name": "@tripetto/runner-fabric",
  "title": "Tripetto Runner Fabric",
  "version": "8.0.16",
  "description": "Collection of UI components for building Tripetto runner UIs.",
  "license": "MIT",
  "private": false,
  "homepage": "https://gitlab.com/tripetto/runner-fabric",
  "keywords": [
    "tripetto"
  ],
  "author": {
    "name": "Mark van den Brink",
    "email": "mark@vandenbr.ink",
    "url": "http://mark.vandenbr.ink"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tripetto/runner-fabric.git"
  },
  "bugs": {
    "url": "https://gitlab.com/tripetto/runner-fabric/issues"
  },
  "main": "./dist/es5/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/es5/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/es5/index.js"
    },
    "./color": {
      "import": "./dist/esm/color/index.mjs",
      "require": "./dist/es5/color/index.js",
      "types": "./dist/types/color/index.d.ts",
      "default": "./dist/es5/color/index.js"
    },
    "./fontloader": {
      "import": "./dist/esm/fontloader/index.mjs",
      "require": "./dist/es5/fontloader/index.js",
      "types": "./dist/types/fontloader/index.d.ts",
      "default": "./dist/es5/fontloader/index.js"
    },
    "./components/*": {
      "import": "./dist/esm/components/*/index.mjs",
      "require": "./dist/es5/components/*/index.js",
      "types": "./dist/types/components/*/index.d.ts",
      "default": "./dist/es5/components/*/index.js"
    },
    "./icons/*": {
      "import": "./dist/esm/icons/*.mjs",
      "require": "./dist/es5/icons/*.js",
      "types": "./dist/types/icons/*.d.ts",
      "default": "./dist/es5/icons/*.js"
    },
    "./overlay": {
      "import": "./dist/esm/overlay/index.mjs",
      "require": "./dist/es5/overlay/index.js",
      "types": "./dist/types/overlay/index.d.ts",
      "default": "./dist/es5/overlay/index.js"
    },
    "./shapes/*": {
      "import": "./dist/esm/shapes/*.mjs",
      "require": "./dist/es5/shapes/*.js",
      "types": "./dist/types/shapes/*.d.ts",
      "default": "./dist/es5/shapes/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "update": "npm-check -su && node ./tasks/update.js",
    "format": "prettier --trailing-comma es5 --write \"src/**/*.ts*\" \"*.json\" \"!package-lock.json\" \"*.yml\" \"webpack.config.js\" \"rollup.config.mjs\"",
    "lint": "eslint ./src/**/*.ts*",
    "stats": "ts-stats",
    "make:clean": "shx rm -rf *.tgz ./dist/* ./color/ ./components/ ./fontloader/ ./icons/ ./shapes/",
    "make:compile": "tsc",
    "make:es5": "tsc --target ES5 --module CommonJS --outDir ./dist/es5/ --noEmit false --pretty --removeComments --sourceMap false && prettier --ignore-path=.prettierignore --trailing-comma es5 --write \"dist/es5/**/*.js\"",
    "make:esm": "rollup -c && node ./tasks/esm.js && prettier --ignore-path=.prettierignore --trailing-comma es5 --write \"dist/esm/**/*.mjs\"",
    "make:types": "tsc --declaration --noEmit false --emitDeclarationOnly --diagnostics --outDir ./dist/types/ && prettier --ignore-path=.prettierignore --trailing-comma es5 --tab-width 4 --write \"dist/types/**/*.d.ts\" && shx cp -R ./dist/types/* ./ && shx rm -rf ./index.d.ts ./fontloader/loader.d.ts ./components/*.d.ts",
    "make:build": "npm run make:clean && npm run make:es5 && npm run make:esm && npm run make:types",
    "make": "npm run lint && npm run make:build && npm run stats",
    "test": "webpack-dev-server --mode development",
    "preversion": "npm run format && npm run lint && npm run make:compile && exit-on-dirty-git",
    "postversion": "git push --follow-tags",
    "prepack": "npm run make",
    "prepublishOnly": "exit-on-dirty-git",
    "publish:next": "git-branch-is next && git push --set-upstream origin next && npm version prerelease && npm publish --tag next",
    "publish:patch": "git-branch-is master && npm version patch && npm publish",
    "publish:minor": "git-branch-is master && npm version minor && npm publish",
    "publish:major": "git-branch-is master && npm version major && npm publish"
  },
  "devDependencies": {
    "@compodoc/ts-stats": "^1.1.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tripetto/runner": "^8.1.0",
    "@types/color-name": "^1.1.3",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "exit-on-dirty-git": "^1.0.1",
    "git-branch-is": "^4.0.0",
    "npm-check": "^6.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.13.0",
    "shx": "^0.3.4",
    "styled-components": "^6.1.8",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "dependencies": {
    "@types/color": "^3.0.6",
    "color": "^3.2.1",
    "webfontloader": "^1.6.28"
  },
  "peerDependencies": {
    "@tripetto/runner": ">= 8.x",
    "react": ">= 16.14.x",
    "styled-components": ">= 5.1.x",
    "tslib": ">= 2.x"
  }
}
